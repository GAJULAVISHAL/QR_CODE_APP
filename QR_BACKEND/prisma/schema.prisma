model Pass {
  id          Int      @id @default(autoincrement())
  userId      String
  personName  String   // Added personName field
  eventName   String
  age         String
  price       Float
  eventDate   String
  bookingDate String
  qrCodeUrl   String
  createdAt DateTime @default(now())
}